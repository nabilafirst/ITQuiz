<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bb"
    android:paddingBottom="35dp"
    android:paddingLeft="8dp"
    android:paddingRight="8dp"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="id.sch.smktelkom_mlg.project.xirpl407162534.itquiz.MainActivity">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="320dp"
        android:layout_height="120dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="54dp"
        android:src="@drawable/itquiz" />

    <ImageView
        android:id="@+id/imageViewAndroid"
        android:layout_width="170dp"
        android:layout_height="70dp"
        android:layout_above="@+id/imageViewDesktop"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/androidbackground" />

    <ImageView
        android:id="@+id/imageViewDesktop"
        android:layout_width="170dp"
        android:layout_height="70dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="60dp"
        android:background="@drawable/desktopbackground" />

    <ImageView
        android:id="@+id/imageViewWeb"
        android:layout_width="170dp"
        android:layout_height="70dp"
        android:layout_alignLeft="@+id/imageViewJava"
        android:layout_alignStart="@+id/imageViewJava"
        android:layout_alignTop="@+id/imageViewAndroid"
        android:background="@drawable/webbackground" />

    <ImageView
        android:id="@+id/imageViewJava"
        android:layout_width="170dp"
        android:layout_height="70dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/imageViewWeb"
        android:background="@drawable/javabackground" />
</RelativeLayout>
